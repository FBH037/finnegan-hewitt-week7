<h1>All users</h1>
<%= link_to "New User", new_user_path %>
<table>
  <thead>
    <tr>
      <th> User Name </th>
      <th> About </th>
    </tr>
  </thead>
  <tbody>
    <% @users.each do |user| %>
      <tr>
        <td> <%= link_to user.user_name, user_path(user) %></td>
        <td><%=user.about%></td>
        <td> <%= link_to "Edit User", edit_user_path(user) %> </td>
        <td> <%= link_to "Delete User", user_path(user), method: :delete %></td>
      </tr>
    <% end %>
  </tbody>
</table>
